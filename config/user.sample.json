{
    "chocolatey" : {
        "packages": [
            { "id": "sourcetree", "disabled": true }
            , { "id": "tortoisegit", "disabled": true }
        ]},
    "vscode":  {
                   "extensions":  [
                                      {
                                          "name":  "ms-vscode.PowerShell"
                                          , "disabled": true
                                      }
                                  ]
                   , "disabled": false
               },
    "drives":  [
                   {
                       "local":  "<drive-letter>",
                       "secret":  {
                                      "remote":  "\\\\<share-host>\\<share-name>"
                                  }
                       , "disabled": true
                   }
               ],
    "vault":  [
                  {
                      "type":  "domain",
                      "secret":  {
                                     "server":  "<hostname>",
                                     "username":  "<domainname>\\<username>",
                                     "password":  "<password>"
                                 }
                       , "disabled": true
                  },
                  {
                      "type":  "generic",
                      "secret":  {
                                     "server":  "git:http://<uri>:<port>",
                                     "username":  "<username>",
                                     "password":  "<password>"
                                 }
                       , "disabled": true
                  },
                  {
                      "type":  "dialup",
                      "secret":  {
                                     "name":  "<connection-name>",
                                     "domain":  "<domainname>",
                                     "username":  "<username>",
                                     "password":  "<password>"
                                 }
                       , "disabled": true
                  }
              ]
}
